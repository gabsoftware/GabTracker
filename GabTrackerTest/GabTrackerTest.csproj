  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|AnyCPU'">
    <CodeAnalysisLogFile>bin\Debug\GabTrackerTest.exe.CodeAnalysisLog.xml</CodeAnalysisLogFile>
